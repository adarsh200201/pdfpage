{
  "name": "pdfpage-backend",
  "version": "1.0.0",
  "description": "Backend API for PdfPage - The Ultimate PDF Toolkit",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "start:dev": "set NODE_ENV=development && node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:schema": "node run-schema-tests.js",
    "test:schema:dev": "ENABLE_TEST_ROUTES=true node run-schema-tests.js",
    "migrate": "node migrate-schema.js",
    "migrate:optional-fields": "node migrate-optional-fields.js",
    "verify": "node verify-database.js",
    "verify:optional-fields": "node verify-optional-fields.js",
    "health": "curl http://localhost:5000/api/health",
    "audit": "npm audit && npm audit fix",
    "security-check": "npm audit --audit-level high",
    "logs:error": "tail -f logs/error.log",
    "logs:combined": "tail -f logs/combined.log"
  },
  "keywords": [
    "pdf",
    "node",
    "express",
    "mongodb"
  ],
  "author": "Your Name",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/vision": "5.2.0",
    "@types/uuid": "10.0.0",
    "archiver": "^7.0.1",
    "aws-sdk": "2.1692.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.40.0",
    "compress-pdf": "0.5.4",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "docx": "^9.5.1",
    "dotenv": "^16.3.1",
    "exceljs": "4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-session": "1.18.1",
    "express-slow-down": "^2.0.1",
    "express-validator": "^7.0.1",
    "glob": "11.0.3",
    "gm": "1.25.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "3.0.1",
    "jspdf-autotable": "5.0.2",
    "jszip": "3.10.1",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-forge": "1.3.1",
    "node-qpdf": "1.0.3",
    "node-tesseract-ocr": "2.2.1",
    "nodemailer": "^6.9.4",
    "officegen": "^0.6.5",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pdf-encrypt": "1.0.1",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pdf-poppler": "0.2.1",
    "pdf2pic": "3.2.0",
    "pptx2json": "0.0.10",
    "razorpay": "^2.9.2",
    "sharp": "^0.34.2",
    "tesseract.js": "6.0.1",
    "uuid": "11.1.0",
    "winston": "^3.17.0",
    "xlsx": "0.18.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
