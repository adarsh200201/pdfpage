# Local Development Environment Variables
# Copy this to .env.local and update values for local development

# Use local backend (runs on npm run backend:dev)
VITE_API_URL=http://localhost:5000/api

# Or use deployed Render backend
# VITE_API_URL=https://pdfpage-backend.onrender.com/api

# Google OAuth
VITE_GOOGLE_CLIENT_ID=YOUR_GOOGLE_CLIENT_ID_HERE
VITE_GOOGLE_CLIENT_SECRET=YOUR_GOOGLE_CLIENT_SECRET_HERE

# Razorpay
VITE_RAZORPAY_KEY_ID=rzp_test_your_key_here

# App Settings
VITE_APP_NAME=PdfPage
VITE_APP_VERSION=1.0.0
VITE_DISABLE_AUTH=false
